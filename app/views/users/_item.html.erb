<% @users = pagination(@users) %>
<% @users.each do |user| %>
<h3><%=user.username%></h3>
<p><%= user.email%></p>
<p>isadmin:<%= user.isadmin%></p>
<%= form_for :user, url:user_path(user.id),method: :patch do |f|%>
<%= f.select :isadmin,[['No',0],['Yes',1]]%>
<md-button class="md-raised" type="submit">admin</md-button>
<%end%>
<%= link_to 'Delete', user_path(user.id),method: :delete, data:{confirm: ' Are you sure'}%><br>
<%end%>
<%= will_paginate @users %>
