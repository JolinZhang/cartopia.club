<h3>the car this user favorite</h3>
<% current_user.favorites.each do |favorite| %>
 <div class="car">
 <div class="col-sm-3 ">
   <div class=""><%= image_tag("/assets/user_car/#{favorite.car.picture}", :alt => "rss feed") %></div>
 </div>
 <%= link_to car_path(favorite.car.id) do %>
 <div class="col-sm-9 ">
   <div class="carseller">post by:<%=favorite.user.username%></div>
   <div class="carcontact">contact:<%=favorite.car.contact%></div>
 </div>
 <%end %>
 </div>
<%end%>
