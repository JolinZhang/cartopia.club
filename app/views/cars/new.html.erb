<div class="main">
<%= form_for :car , url: cars_path do |f| %>


  <p>
    <%= f.label :year %>
    <%= f.select :year,[['2016','2016'],['2015','2015'],['2014','2014'],
    ['2013','2013'],['2012','2012'],['2011','2011'],['2010','2010'],
    ['2009','2009'],['2008','2008'],['2007','2007'],['2006','2006'],
    ['2005','2005'],['2004','2004'],['2003','2003'],['2002','2002'],
    ['2001','2001'],['2000','2000'],['1999','1999'],['1998','1998'],
    ['1997','1997'],['1996','1996'],['1995','1995']]%>
    <%= f.label :make %>
    <%= f.select :make,[['All Makes','All Makes'],['Acura','Acura'],['Alfa Romeo','Alfa Romeo'],
    ['Am General','Am General'],['Aston Martin','Aston Martin'],['Audi','Audi']] %>
    <%= f.label :model %>
    <%= f.text_field :model %>*
    <%= f.label :mileage %>
    <%= f.text_field :mileage %>*
    <%= f.label :price %>
    <%= f.text_field :price %>*

  </p>
  <p>
    <%= f.label :state %>
    <%= f.text_field :state %>*
    <%= f.label :city %>
    <%= f.text_field :city %>*
    <%= f.label :contact %>
    <%= f.text_field :contact %>*
  </p>
  <p>
    <%= f.label :notes %>
    <%= f.text_area :notes,cols: "100", rows: "5"%>
  </p>
  <p>
    <%= f.file_field :picture%>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>


<% if @car.errors.any? %>
   <div id="error_explanation">
     <h2>
       <%= pluralize(@car.errors.count, "error") %> prohibited
       this car from being saved:
     </h2>
     <ul>
       <% @car.errors.full_messages.each do |msg| %>
         <li><%= msg %></li>
       <% end %>
     </ul>
   </div>
 <% end %>

</div>
