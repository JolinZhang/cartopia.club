<div layout="row">

<div flex="10" hide-xs></div>

<div layout="column" flex>
	<md-card>
		<md-card-content>
			<%= form_for :car , url: cars_path do |f| %>

			<div layout="row">

			<md-input-container flex>
        <label>Year</label>
        <md-select ng-model="year">
          <md-option value="2016">2016</md-option>
					<md-option value="2015">2015</md-option>
					<md-option value="2014">2014</md-option>
					<md-option value="2013">2013</md-option>
					<md-option value="2012">2012</md-option>
					<md-option value="2011">2011</md-option>
					<md-option value="2010">2010</md-option>
					<md-option value="2009">2009</md-option>
					<md-option value="2008">2008</md-option>
					<md-option value="2007">2007</md-option>
					<md-option value="2006">2006</md-option>
					<md-option value="2005">2005</md-option>
					<md-option value="2004">2004</md-option>
					<md-option value="2003">2003</md-option>
					<md-option value="2002">2002</md-option>
					<md-option value="2001">2001</md-option>
					<md-option value="2000">2000</md-option>
					<md-option value="1999">1999</md-option>
					<md-option value="1998">1998</md-option>
					<md-option value="1997">1997</md-option>
					<md-option value="1996">1996</md-option>
					<md-option value="1995">1995</md-option>
					<md-option value="1994">1994</md-option>
					<md-option value="1993">1993</md-option>
        </md-select>
      </md-input-container>


			<md-input-container flex>
        <label>Make</label>
        <md-select ng-model="make">
          <md-option value="All Makes">All Makes</md-option>
				  <md-option value="Acura">Acura</md-option>
					<md-option value="Alfa Romeo">Alfa Romeo</md-option>
				  <md-option value="Am General">Am General</md-option>
					<md-option value="Aston Martin">Aston Martin</md-option>
				  <md-option value="Audi">Audi</md-option>
					<md-option value="Avanti Motors">Avanti Motors</md-option>
				  <md-option value="Bentley">Bentley</md-option>
					<md-option value="BMW">BMW</md-option>
				  <md-option value="Bugatti">Bugatti</md-option>
					<md-option value="Buick">Buick</md-option>
				  <md-option value="Cadillac">Cadillac</md-option>
					<md-option value="Chevrolet">Chevrolet</md-option>
				  <md-option value="Chrysler">Chrysler</md-option>
					<md-option value="Daewoo">Daewoo</md-option>
				  <md-option value="Dodge">Dodge</md-option>
					<md-option value="Ferrari">Ferrari</md-option>
				  <md-option value="FIAT">FIAT</md-option>
					<md-option value="Fisker">Fisker</md-option>
				  <md-option value="Ford">Ford</md-option>
					<md-option value="GMC">GMC</md-option>
				  <md-option value="Honda">Honda</md-option>
					<md-option value="Hummer">Hummer</md-option>
				  <md-option value="Hyundai">Hyundai</md-option>
					<md-option value="Infiniti">Infiniti</md-option>
				  <md-option value="International">International</md-option>
					<md-option value="Isuzu">Isuzu</md-option>
				  <md-option value="Jaguar">Jaguar</md-option>
					<md-option value="Jeep">Jeep</md-option>
				  <md-option value="Kia">Kia</md-option>
					<md-option value="Koenigsegg">Koenigsegg</md-option>
					<md-option value="Lamborghini">Lamborghini</md-option>
					<md-option value="Land Rover">Land Rover</md-option>
					<md-option value="Lexus">Lexus</md-option>
					<md-option value="Lincoln">Lincoln</md-option>
					<md-option value="Lotus">Lotus</md-option>
					<md-option value="Maserati">Maserati</md-option>
					<md-option value="Maybach">Maybach</md-option>
					<md-option value="Mazda">Mazda</md-option>
					<md-option value="McLaren">McLaren</md-option>
					<md-option value="Mercedes-Benz">Mercedes-Benz</md-option>
					<md-option value="Mercury">Mercury</md-option>
					<md-option value="MINI">MINI</md-option>
					<md-option value="Mitsubishi">Mitsubishi</md-option>
					<md-option value="Morgan">Morgan</md-option>
					<md-option value="Nissan">Nissan</md-option>
					<md-option value="Oldsmobile">Oldsmobile</md-option>
					<md-option value="Panoz">Panoz</md-option>
					<md-option value="Plymouth">Plymouth</md-option>
					<md-option value="Pontiac">Pontiac</md-option>
					<md-option value="Porsche">Porsche</md-option>
					<md-option value="Qvale">Qvale</md-option>
					<md-option value="RAM">RAM</md-option>
					<md-option value="Rolls-Royce">Rolls-Royce</md-option>
					<md-option value="Saab">Saab</md-option>
					<md-option value="Saleen">Saleen</md-option>
					<md-option value="Saturn">Saturn</md-option>
					<md-option value="Scion">Scion</md-option>
					<md-option value="smart">smart</md-option>
					<md-option value="Spyker">Spyker</md-option>
					<md-option value="Subaru">Subaru</md-option>
					<md-option value="Suzuki">Suzuki</md-option>
					<md-option value="Tesla">Tesla</md-option>
					<md-option value="Toyota">Toyota</md-option>
					<md-option value="Volkswagen">Volkswagen</md-option>
					<md-option value="Volvo">Volvo</md-option>
        </md-select>
      </md-input-container>


			<md-input-container flex="50">
				<label>Mode</label>
				<%= f.text_field :model %><br>
			</md-input-container>

			</div>

			<div layout="row">
			<md-input-container flex="50">
				<label>Mileage</label>
				<%= f.text_field :mileage %><br>
			</md-input-container>
			<md-input-container flex="50">
				<label>Price</label>
				<%= f.text_field :price %><br>
			</md-input-container>

			</div>

			<div layout="row">
			<md-input-container flex="50">
        <label>State</label>
        <md-select ng-model="state">
          <md-option value="TX">TX</md-option>
        </md-select>
      </md-input-container>

			<md-input-container flex="50">
				<label>City</label>
				<%= f.text_field :city %><br>
			</md-input-container>

			</div>

			<md-input-container class="md-block">
          <label>Notes</label>
          <textarea ng-model="notes" md-maxlength="256" rows="5" md-select-on-focus></textarea>
			</md-input-container>

			<div layout="row">
			<%= f.file_field :picture, :style =>'display:none' %>
			<label for="car_picture" class="md-button md-raised">Choose Files</label>

			<div flex></div>

			<md-button class="md-raised" type="submit">SUBMIT</md-button>

			</div>

			<% end %>
		</md-card-content>
	</md-card>
</div>

<div flex="10" hide-xs></div>

</div>
